        <if test="comCode != null">
			 and com_code=#{comCode}
		 </if>
		 <if test="moduCode != null">
			 and modu_code=#{moduCode}
		 </if>
            <if test="id != null">
                and ID=#{id}
            </if>
            <if test="rpPositionName != null">
                and rp_position_name=#{rpPositionName}
            </if>
            <if test="rpPositionDesc != null">
                and rp_position_desc=#{rpPositionDesc}
            </if>
            <if test="comId != null">
                and com_id=#{comId}
            </if>
            <if test="createTime != null">
