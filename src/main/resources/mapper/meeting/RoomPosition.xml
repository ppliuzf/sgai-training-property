    <select id="findRoomPositionListExistsRoom" resultMap="BaseResultMap">
        select DISTINCT rp.ID, rp.rp_position_name, rp.rp_position_desc, rp.com_id, rp.create_time,
        rp.update_time, rp.is_delete from mt_room_position rp,mt_room_resource rr where
        rr.rp_id = rp.ID and rr.is_delete = 0 and rp.is_delete = 0
        <if test="currtDate != null and timeSeg!=null ">
            and not EXISTS (select 1 from mt_maininfo mi where mi.rr_id = rr.id and
            mi.mi_status IN(1,2)
            and mi.is_delete = 0
            <if test="createEiId != null ">
                and mi.create_ei_id != #{createEiId}
            </if>
            AND mi.mi_mt_date = #{currtDate}
            <!-- and splitTimeSeg(#{timeSeg},mi.mi_mt_time_seg) > 0
             AND	mi.mi_mt_time_seg LIKE CONCAT('%',#{timeSeg},'%') -->
            )
        </if>
    </select>
